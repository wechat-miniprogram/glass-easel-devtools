>
  <block wx:if="{{ !computedStyleCollapsed && computedStyles }}">
    <view wx:for="{{ computedStyles }}" class="section-item">
      <view class="section-key">{{ item.name }}</view>: <view class="section-value">{{ item.value }}</view>
    </view>
  </block>
</detail-section>
